        <CommandText>SELECT 
B1P.B1_ALT_ID AS Docket_No,
BCK.Ref_No AS Ref_No,
BCK.Enforcement_Type,
--BCK.Revised_Fine_Amount,
CASE WHEN BCK.Enforcement_Type IN ('OP')
		THEN ''
		ELSE BCK.Revised_Fine_Amount END AS Revised_Fine_Amount ,
GP.B1_DUE_DD AS Due_Date,

CASE WHEN BCK.Enforcement_Type IN ('OP','EE/TT/SP/LL/BB','EE','TT','SP','LL','BB')
     THEN ' and all violations will have been corrected by that same date. '
     WHEN BCK.Enforcement_Type IN('T8','IW') 
	 THEN ' In addition, I agree to comply with the Corrective Actions listed in Appendix “A” and Terms and Conditions set forth in the Notice of Preliminary Hearing by the dates indicated therein. '
     ELSE ' '
END AS Enf_txt,
CASE WHEN BCK.Enforcement_Type IN ('OP')
     THEN ' $250.00 plus overdue Operating Permit fee of '
     ELSE ' '
END AS Enf_OP,

CASE WHEN BCK.Enforcement_Type IN ('EE/TT/SP/LL/BB','EE','TT','SP','LL','BB','T8','IW')
     THEN ''
	  ELSE CONVERT(VARCHAR,BLC.DUE_FEE)
END AS OP_AMOUNT,

CASE WHEN BCK.Enforcement_Type IN ('EE/TT/SP/LL/BB','EE','TT','SP','LL','BB','T8','IW')
     THEN ''
	  ELSE (CASE WHEN BLC.DUE_FEE = 0.00 
	  THEN'0.00'
	  ELSE CONVERT(VARCHAR,BLC.DUE_FEE+250)
	  END)
END AS OP_AMOUNT250,

COALESCE(BCKSP.B1_CHECKLIST_COMMENT,'') AS SITE_PRT_ID,
GP.SD_APP_DES,
GP.SD_PRO_DES

FROM B1PERMIT B1P
LEFT JOIN
(SELECT B1P2.SERV_PROV_CODE, B1P2.B1_PER_ID1, B1P2.B1_PER_ID2, B1P2.B1_PER_ID3,
MAX(CASE WHEN B1_CHECKBOX_DESC = 'Enforcement Type' THEN B1_CHECKLIST_COMMENT END) AS 'Enforcement_Type',
MAX(CASE WHEN B1_CHECKBOX_DESC = 'Revised Fine Amount' THEN B1_CHECKLIST_COMMENT END) AS 'Revised_Fine_Amount',
MAX(CASE WHEN B1_CHECKBOX_DESC = 'File Reference Number/Facility ID' THEN B1_CHECKLIST_COMMENT END) AS 'Ref_No'
FROM BCHCKBOX BCK1
JOIN B1PERMIT B1P2
ON BCK1.SERV_PROV_CODE = B1P2.SERV_PROV_CODE
AND BCK1.B1_PER_ID1 = B1P2.B1_PER_ID1
AND BCK1.B1_PER_ID2 = B1P2.B1_PER_ID2
AND BCK1.B1_PER_ID3 = B1P2.B1_PER_ID3
WHERE B1P2.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1P2.B1_PER_GROUP = 'DEQ'
AND B1P2.B1_PER_TYPE = 'OPC'
AND B1P2.B1_PER_SUB_TYPE = 'Enforcement'
AND B1P2.B1_PER_CATEGORY = 'NA'
GROUP BY B1P2.SERV_PROV_CODE, B1P2.B1_PER_ID1, B1P2.B1_PER_ID2, B1P2.B1_PER_ID3
) BCK
ON BCK.SERV_PROV_CODE = B1P.SERV_PROV_CODE
AND BCK.B1_PER_ID1 = B1P.B1_PER_ID1
AND BCK.B1_PER_ID2 = B1P.B1_PER_ID2
AND BCK.B1_PER_ID3 = B1P.B1_PER_ID3
AND BCK.Enforcement_Type IN ('EE/TT/SP/LL/BB','EE','TT','SP','LL','BB','T8','IW','OP')

JOIN GPROCESS GP
ON GP.SERV_PROV_CODE = B1P.SERV_PROV_CODE
AND GP.B1_PER_ID1 = B1P.B1_PER_ID1
AND GP.B1_PER_ID2 = B1P.B1_PER_ID2
AND GP.B1_PER_ID3 = B1P.B1_PER_ID3
AND GP.SD_PRO_DES IN ('Preliminary Hearing','Formal Hearing')
AND GP.SD_APP_DES = 'Revised Waiver'

LEFT JOIN BCHCKBOX BCKSP
ON B1P.SERV_PROV_CODE = BCKSP.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCKSP.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCKSP.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCKSP.B1_PER_ID3
AND BCKSP.B1_CHECKBOX_DESC = 'Site/Pool (Parent) Record ID'
JOIN (SELECT B1PB.SERV_PROV_CODE, B1PB.B1_PER_ID1, B1PB.B1_PER_ID2, B1PB.B1_PER_ID3,
SUM(F4I.BALANCE_DUE) AS DUE_FEE,SUM(F4I.INVOICE_AMOUNT) AS TOTAL_FEE 
FROM B1PERMIT B1PB

LEFT JOIN XAPP2REF XAR
ON B1PB.SERV_PROV_CODE = XAR.SERV_PROV_CODE
AND B1PB.B1_PER_ID1 = XAR.B1_PER_ID1
AND B1PB.B1_PER_ID2 = XAR.B1_PER_ID2
AND B1PB.B1_PER_ID3 = XAR.B1_PER_ID3

LEFT JOIN B1PERMIT B1PSP
ON B1PSP.SERV_PROV_CODE = XAR.SERV_PROV_CODE
AND B1PSP.B1_PER_ID1 = XAR.B1_MASTER_ID1
AND B1PSP.B1_PER_ID2 = XAR.B1_MASTER_ID2
AND B1PSP.B1_PER_ID3 = XAR.B1_MASTER_ID3
AND B1PSP.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1PSP.B1_PER_GROUP = 'DEQ'
AND B1PSP.B1_PER_TYPE = 'General'
AND B1PSP.B1_PER_SUB_TYPE = 'Site'
AND B1PSP.B1_PER_CATEGORY = 'NA'

LEFT JOIN F4FEEITEM F4F
ON F4F.SERV_PROV_CODE=B1PSP.SERV_PROV_CODE
AND F4F.B1_PER_ID1=B1PSP.B1_PER_ID1
AND F4F.B1_PER_ID2=B1PSP.B1_PER_ID2
AND F4F.B1_PER_ID3=B1PSP.B1_PER_ID3

LEFT JOIN X4FEEITEM_INVOICE X4I
ON X4I.SERV_PROV_CODE=B1PSP.SERV_PROV_CODE
AND X4I.B1_PER_ID1=B1PSP.B1_PER_ID1
AND X4I.B1_PER_ID2=B1PSP.B1_PER_ID2
AND X4I.B1_PER_ID3=B1PSP.B1_PER_ID3
AND X4I.FEEITEM_SEQ_NBR=F4F.FEEITEM_SEQ_NBR
AND X4I.GF_DES = 'OPC Site - Permit to Operate'

LEFT JOIN F4INVOICE F4I 
ON F4I.SERV_PROV_CODE=B1PSP.SERV_PROV_CODE
AND X4I.SERV_PROV_CODE=F4I.SERV_PROV_CODE
AND X4I.INVOICE_NBR =F4I.INVOICE_NBR
GROUP BY B1PB.SERV_PROV_CODE, B1PB.B1_PER_ID1, B1PB.B1_PER_ID2, B1PB.B1_PER_ID3
) 
AS BLC

ON BLC.SERV_PROV_CODE = B1P.SERV_PROV_CODE
AND BLC.B1_PER_ID1 = B1P.B1_PER_ID1
AND BLC.B1_PER_ID2 = B1P.B1_PER_ID2
AND BLC.B1_PER_ID3 = B1P.B1_PER_ID3

WHERE B1P.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1P.B1_PER_GROUP = 'DEQ'
AND B1P.B1_PER_TYPE = 'OPC'
AND B1P.B1_PER_SUB_TYPE = 'Enforcement'
AND B1P.B1_PER_CATEGORY = 'NA'
--AND B1P.B1_ALT_ID ='ENF-22-00024-EE'
AND B1P.B1_ALT_ID = @RecordID</CommandText>
