    <DataSet Name="ASIT">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RecordID">
            <Value>=Parameters!RecordID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',BCK.B1_CHECKLIST_COMMENT)AS Inspectors_Observations,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',BCK1.B1_CHECKLIST_COMMENT)AS Responsible_Individuals, 
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',BCK2.B1_CHECKLIST_COMMENT)AS Inspector_Recommendation ,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',BCK3.B1_CHECKLIST_COMMENT)AS Information_Related_to_Complainant 

FROM B1PERMIT B1P

LEFT JOIN BCHCKBOX BCK
ON B1P.SERV_PROV_CODE = BCK.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK.B1_PER_ID3
AND BCK.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK.B1_CHECKBOX_DESC = 'Inspector Observations'

LEFT JOIN BCHCKBOX BCK1
ON B1P.SERV_PROV_CODE = BCK1.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK1.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK1.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK1.B1_PER_ID3
AND BCK1.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK1.B1_CHECKBOX_DESC = 'Responsible Individual'

LEFT JOIN BCHCKBOX BCK2
ON B1P.SERV_PROV_CODE = BCK2.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK2.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK2.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK2.B1_PER_ID3
AND BCK2.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK2.B1_CHECKBOX_DESC = 'Inspector Recommendation'

LEFT JOIN BCHCKBOX BCK3
ON B1P.SERV_PROV_CODE = BCK3.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK3.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK3.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK3.B1_PER_ID3
AND BCK3.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK3.B1_CHECKBOX_DESC = 'Information Related to Complainant'


WHERE B1P.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1P.B1_PER_GROUP = 'DEQ'
AND B1P.B1_PER_TYPE = 'General'
AND B1P.B1_PER_SUB_TYPE = 'Complaint'
AND B1P.B1_PER_CATEGORY = 'NA'
AND B1P.B1_ALT_ID = @RecordID
--AND B1P.B1_ALT_ID = 'COMP-22-0017'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Inspectors_Observations">
          <DataField>Inspectors_Observations</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Responsible_Individuals">
          <DataField>Responsible_Individuals</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Inspector_Recommendation">
          <DataField>Inspector_Recommendation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
