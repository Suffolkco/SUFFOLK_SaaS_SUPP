    <DataSet Name="Interviews">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RecordID">
            <Value>=Parameters!RecordID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*DECLARE @RecordID VARCHAR(100) 
SET @RecordID ='COMP-23-0009'*/


SELECT DISTINCT

CONCAT('#',BAPV.Interviewee_ID, ' ', BAPV.Interviewee_Name) AS Interviewee_ID,
BAPV.Persons_Interviewed AS Persons_Interviewed,


CONCAT(BAPV.Street_Address, ' ', BAPV.Town,' ' ,BAPV.Zip_Code) AS Interviews_Address,
COALESCE(BAPV.Interviewee_Phone,'') AS Interviewee_Phone,
CONCAT(BAPV.Interviewee_ID, ': ',BAPV.Information_Obtained) AS Information_Obtained

FROM B1PERMIT B1P


LEFT JOIN GPROCESS_HISTORY GH ON 
B1P.SERV_PROV_CODE = GH.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = GH.B1_PER_ID1
AND B1P.B1_PER_ID2 = GH.B1_PER_ID2
AND B1P.B1_PER_ID3 = GH.B1_PER_ID3
AND GH.SD_PRO_DES = 'Complaint Received'
AND GH.SD_APP_DES = 'Investigation Underway'




LEFT JOIN
(SELECT BAPV1.SERV_PROV_CODE, BAPV1.B1_PER_ID1, BAPV1.B1_PER_ID2, BAPV1.B1_PER_ID3, BAPV1.ROW_INDEX,
MAX(CASE WHEN BAPV1.COLUMN_NAME ='Person Interviewed'THEN BAPV1.ATTRIBUTE_VALUE END) AS Persons_Interviewed,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Interviewee ID' THEN BAPV1.ATTRIBUTE_VALUE END) AS Interviewee_ID,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Name' THEN BAPV1.ATTRIBUTE_VALUE END) AS Interviewee_Name,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Street Address' THEN BAPV1.ATTRIBUTE_VALUE END) AS Street_Address,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Town' THEN BAPV1.ATTRIBUTE_VALUE END) AS Town,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Zip Code' THEN BAPV1.ATTRIBUTE_VALUE END) AS Zip_Code,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Phone' THEN BAPV1.ATTRIBUTE_VALUE END) AS Interviewee_Phone,
MAX(CASE WHEN BAPV1.COLUMN_NAME='Information Obtained' THEN BAPV1.ATTRIBUTE_VALUE END) AS Information_Obtained

FROM B1PERMIT B1P1
JOIN BAPPSPECTABLE_VALUE BAPV1
ON BAPV1.SERV_PROV_CODE = B1P1.SERV_PROV_CODE
AND BAPV1.B1_PER_ID1 = B1P1.B1_PER_ID1
AND BAPV1.B1_PER_ID2 = B1P1.B1_PER_ID2
AND BAPV1.B1_PER_ID3 = B1P1.B1_PER_ID3
AND BAPV1.TABLE_NAME ='INTERVIEWS'
WHERE B1P1.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1P1.B1_PER_GROUP = 'DEQ'
AND B1P1.B1_PER_TYPE = 'General'
AND B1P1.B1_PER_SUB_TYPE = 'Complaint'
AND B1P1.B1_PER_CATEGORY = 'NA'
AND B1P1.B1_ALT_ID = @RecordID
GROUP BY BAPV1.SERV_PROV_CODE, BAPV1.B1_PER_ID1, BAPV1.B1_PER_ID2, BAPV1.B1_PER_ID3, BAPV1.ROW_INDEX
) BAPV

ON BAPV.SERV_PROV_CODE = B1P.SERV_PROV_CODE
AND BAPV.B1_PER_ID1 = B1P.B1_PER_ID1
AND BAPV.B1_PER_ID2 = B1P.B1_PER_ID2
AND BAPV.B1_PER_ID3 = B1P.B1_PER_ID3

WHERE B1P.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1P.B1_PER_GROUP = 'DEQ'
AND B1P.B1_PER_TYPE = 'General'
AND B1P.B1_PER_SUB_TYPE = 'Complaint'
AND B1P.B1_PER_CATEGORY = 'NA'
AND B1P.B1_ALT_ID = @RecordID
--AND B1P.B1_ALT_ID = 'COMP-22-0017'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
