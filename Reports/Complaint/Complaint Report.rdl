        <CommandText>/*DECLARE @RecordID VARCHAR(100) 
SET @RecordID ='COMP-23-0001'*/

SELECT DISTINCT B1P.B1_ALT_ID AS SCDHS_No,
Sig.logo_content AS Signat,
sig.LOGO_DESC AS InspName,
CONVERT(VARCHAR, GETDATE(), 32) AS CUR_DT,
BCK.B1_CHECKLIST_COMMENT AS Intake_Source,
CASE WHEN B3C.B1_FNAME IS NULL 
	 THEN B3C.B1_FULL_NAME
	 ELSE B3C.B1_FNAME +' '+ B3C.B1_LNAME
END AS Complainant,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',B3C1.B1_ADDRESS1 )AS Complainant_Address,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',B3C1.B1_CITY )AS Complainant_City,
B3C1.B1_PHONE1 AS Complainant_Phone1,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',BCK1.B1_CHECKLIST_COMMENT) AS Complaint_Against,
--BCK1.B1_CHECKLIST_COMMENT AS TTTTT,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',CONCAT(B3A.B1_HSE_NBR_START, ' ',B3A.B1_STR_NAME))AS Complaint_Against_Address,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',B3A.B1_SITUS_CITY) AS T_V_H,
[dbo].[FN_GET_INITCAP]('SUFFOLKCO',B3A.B1_CROSS_STREET_NAME_START) AS Complaint_Against_Cross,
STUFF(( SELECT DISTINCT  ', ' + CAST(COALESCE((CASE WHEN BCKIN.B1_CHECKBOX_DESC = 'Other_Desc'
THEN BCKIN.B1_CHECKLIST_COMMENT ELSE BCKIN.B1_CHECKBOX_DESC END ),'')AS varchar)
        		FROM BCHCKBOX BCKIN				
				JOIN B1PERMIT B1PIN
				ON B1PIN.SERV_PROV_CODE = BCKIN.SERV_PROV_CODE
				AND B1PIN.B1_PER_ID1 = BCKIN.B1_PER_ID1
				AND B1PIN.B1_PER_ID2 = BCKIN.B1_PER_ID2
				AND B1PIN.B1_PER_ID3 = BCKIN.B1_PER_ID3
				AND BCKIN.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
				AND BCKIN.B1_CHECKBOX_DESC IN ('Discolored Water','Fish Kill','Noise','Odor',
				'OPC - Pesticide/Fertilizer Application','OPC - Petroleum Odors',
				'OPC - Spills of Petroleum Products/Chemicals/Mercury',
				'OPC - Swimming Pools','OPC - Toxic and Hazardous Material Discharge',
				'OPC - Toxic and Hazardous Material Storage Tank','Other_Desc',
				'Overflow','Sewer Blockage','Spill','Surface Water Pollution',
				'WWM - Construction of a Permanent Building w/o a Permit',
				'WWM - Installation of Sanitary System w/o a Permit or Notification of SHIP',
				'WWM - Occupying/Illegal Use of a Sanitary System w/o a Permit')
				AND COALESCE(BCKIN.B1_CHECKLIST_COMMENT,'') &lt;&gt; ''
				WHERE B1P.SERV_PROV_CODE = B1PIN.SERV_PROV_CODE
				AND B1P.B1_PER_ID1 = B1PIN.B1_PER_ID1
				AND B1P.B1_PER_ID2 = B1PIN.B1_PER_ID2
				AND B1P.B1_PER_ID3 = B1PIN.B1_PER_ID3
              FOR
                XML PATH('')
              ), 1, 1, '') AS Nature_of_Request,

CONCAT(GH.G6_ISS_FNAME, ' ', GH.G6_ISS_LNAME) AS RCV_D,
CONCAT(GH1.ASGN_FNAME, ' ', GH1.ASGN_LNAME) AS Assigned_To,
--CONCAT(GH4.ASGN_FNAME, ' ', GH4.ASGN_LNAME) AS Assigned_2,
CONCAT(GH2.ASGN_FNAME, ' ', GH2.ASGN_LNAME) AS Assigned_2,
GH2.SD_APP_DD AS DATE2,
COALESCE(GH1.SD_APP_DD,'') AS NOR_Date

FROM B1PERMIT B1P

LEFT JOIN BCHCKBOX BCK
ON B1P.SERV_PROV_CODE = BCK.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK.B1_PER_ID3
--AND BCK.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK.B1_CHECKBOX_DESC = 'Source of Complaint'

LEFT JOIN B3CONTACT B3C
ON B3C.SERV_PROV_CODE = B1P.SERV_PROV_CODE
AND B3C.B1_PER_ID1 = B1P.B1_PER_ID1
AND B3C.B1_PER_ID2 = B1P.B1_PER_ID2
AND B3C.B1_PER_ID3 = B1P.B1_PER_ID3
AND B3C.B1_CONTACT_TYPE ='Complainant'

LEFT JOIN B3CONTACT B3C1
ON B3C1.SERV_PROV_CODE = B1P.SERV_PROV_CODE
AND B3C1.B1_PER_ID1 = B1P.B1_PER_ID1
AND B3C1.B1_PER_ID2 = B1P.B1_PER_ID2
AND B3C1.B1_PER_ID3 = B1P.B1_PER_ID3

LEFT JOIN BCHCKBOX BCK1
ON B1P.SERV_PROV_CODE = BCK1.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK1.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK1.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK1.B1_PER_ID3
AND BCK1.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK1.B1_CHECKBOX_DESC = 'Facility Name'

LEFT JOIN BCHCKBOX BCK2
ON B1P.SERV_PROV_CODE = BCK2.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = BCK2.B1_PER_ID1
AND B1P.B1_PER_ID2 = BCK2.B1_PER_ID2
AND B1P.B1_PER_ID3 = BCK2.B1_PER_ID3
AND BCK2.B1_CHECKBOX_TYPE = 'ADDITIONAL COMPLAINT DETAILS'
AND BCK2.B1_CHECKBOX_DESC = 'Inspector Observations'

LEFT JOIN B3ADDRES B3A ON 
B1P.SERV_PROV_CODE = B3A.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = B3A.B1_PER_ID1
AND B1P.B1_PER_ID2 = B3A.B1_PER_ID2
AND B1P.B1_PER_ID3 = B3A.B1_PER_ID3
AND COALESCE(B3A.B1_PRIMARY_ADDR_FLG,'Y') = 'Y'

LEFT JOIN (SELECT B1.SERV_PROV_CODE ,B1.B1_PER_ID1,B1.B1_PER_ID2,B1.B1_PER_ID3,
GH1.G6_ISS_FNAME,GH1.G6_ISS_LNAME,ROW_NUMBER() OVER(PARTITION BY B1.SERV_PROV_CODE ,B1.B1_PER_ID1,B1.B1_PER_ID2,B1.B1_PER_ID3
ORDER BY GH1.REC_DATE DESC ) AS RNK 
FROM B1PERMIT B1 
JOIN GPROCESS_HISTORY GH1 ON 
B1.SERV_PROV_CODE = GH1.SERV_PROV_CODE
AND B1.B1_PER_ID1 = GH1.B1_PER_ID1
AND B1.B1_PER_ID2 = GH1.B1_PER_ID2
AND B1.B1_PER_ID3 = GH1.B1_PER_ID3
AND GH1.SD_PRO_DES = 'Complaint Received'
AND GH1.SD_APP_DES = 'Investigation Underway' 
AND B1.B1_ALT_ID = @RecordID ) GH 
ON B1P.SERV_PROV_CODE = GH.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = GH.B1_PER_ID1
AND B1P.B1_PER_ID2 = GH.B1_PER_ID2
AND B1P.B1_PER_ID3 = GH.B1_PER_ID3
AND GH.RNK = 1 

LEFT JOIN (SELECT B1.SERV_PROV_CODE ,B1.B1_PER_ID1,B1.B1_PER_ID2,B1.B1_PER_ID3,GH3.SD_APP_DD,
GH3.ASGN_FNAME,GH3.ASGN_LNAME,ROW_NUMBER() OVER(PARTITION BY B1.SERV_PROV_CODE ,B1.B1_PER_ID1,B1.B1_PER_ID2,B1.B1_PER_ID3
ORDER BY GH3.REC_DATE DESC ) AS RNK 
FROM B1PERMIT B1 
JOIN GPROCESS_HISTORY GH3 ON 
B1.SERV_PROV_CODE = GH3.SERV_PROV_CODE
AND B1.B1_PER_ID1 = GH3.B1_PER_ID1
AND B1.B1_PER_ID2 = GH3.B1_PER_ID2
AND B1.B1_PER_ID3 = GH3.B1_PER_ID3
AND GH3.SD_PRO_DES = 'Investigation'
AND B1.B1_ALT_ID =  @RecordID ) GH2 
ON B1P.SERV_PROV_CODE = GH2.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = GH2.B1_PER_ID1
AND B1P.B1_PER_ID2 = GH2.B1_PER_ID2
AND B1P.B1_PER_ID3 = GH2.B1_PER_ID3
AND GH2.RNK = 1 
LEFT JOIN (SELECT B1.SERV_PROV_CODE ,B1.B1_PER_ID1,B1.B1_PER_ID2,B1.B1_PER_ID3,GH2.SD_APP_DD,
GH2.ASGN_FNAME,GH2.ASGN_LNAME,ROW_NUMBER() OVER(PARTITION BY B1.SERV_PROV_CODE ,B1.B1_PER_ID1,B1.B1_PER_ID2,B1.B1_PER_ID3
ORDER BY GH2.REC_DATE DESC ) AS RNK 
FROM B1PERMIT B1 
JOIN GPROCESS_HISTORY GH2 ON 
B1.SERV_PROV_CODE = GH2.SERV_PROV_CODE
AND B1.B1_PER_ID1 = GH2.B1_PER_ID1
AND B1.B1_PER_ID2 = GH2.B1_PER_ID2
AND B1.B1_PER_ID3 = GH2.B1_PER_ID3
AND GH2.SD_PRO_DES = 'Investigation Review'
AND GH2.SD_APP_DES='Closed'
AND B1.B1_ALT_ID =  @RecordID )GH1
ON B1P.SERV_PROV_CODE = GH1.SERV_PROV_CODE
AND B1P.B1_PER_ID1 = GH1.B1_PER_ID1
AND B1P.B1_PER_ID2 = GH1.B1_PER_ID2
AND B1P.B1_PER_ID3 = GH1.B1_PER_ID3
AND GH1.RNK = 1 

LEFT JOIN G3STAFFS G3S
ON G3S.SERV_PROV_CODE = GH1.SERV_PROV_CODE
AND G3S.GA_FNAME + ' ' + G3S.GA_LNAME =GH1.ASGN_FNAME + ' ' + GH1.ASGN_LNAME

LEFT OUTER JOIN RSERV_LOGO Sig ON 
Sig.SERV_PROV_CODE = B1P.SERV_PROV_CODE
and LOGO_TYPE = LTRIM(RTRIM(
CASE WHEN LEN(LTRIM(RTRIM(COALESCE(G3S.GA_FNAME,'') ))) &gt; 0 THEN LTRIM(RTRIM(COALESCE(G3S.GA_FNAME,'') )) ELSE '' END +
CASE WHEN LEN(LTRIM(RTRIM(COALESCE(G3S.GA_LNAME,'')))) &gt; 0 THEN ' ' + LTRIM(RTRIM(COALESCE(G3S.GA_LNAME,''))) ELSE '' END
))
-------------------------------------------------------------
WHERE B1P.SERV_PROV_CODE = 'SUFFOLKCO'
AND B1P.B1_PER_GROUP = 'DEQ'
AND B1P.B1_PER_TYPE = 'General'
AND B1P.B1_PER_SUB_TYPE = 'Complaint'
AND B1P.B1_PER_CATEGORY = 'NA'
AND B1P.B1_ALT_ID = @RecordID
--AND B1P.B1_ALT_ID = 'COMP-22-0016'</CommandText>
